@model AccountDetails.Context.accdetail
@{
    ViewBag.Title = "AccountDetails";
}

<h2>AccountDetails</h2>
@using (Html.BeginForm("AddAccountDetails", "AccountDetails", FormMethod.Post))
{
<div class="container">
    <div class="form-group">
        <label>FirstName</label>
        @Html.TextBoxFor(x => x.FirstName)
    </div>
    <div class="form-group">
        <label>LastName</label>
        @Html.TextBoxFor(x => x.LastName)
    </div>
    <div class="form-group">
        <label>AccountNumber</label>
        @Html.TextBoxFor(x => x.AccountNumber)
    </div>
    <div class="form-group">
        <label>PAN</label>
        @Html.TextBoxFor(x => x.PAN)
    </div>
    <div class="form-group">
        <label>BankName</label>
        @Html.TextBoxFor(x => x.BankName)
    </div>
    <div class="form-group">
        <label>IFSC</label>
        @Html.TextBoxFor(x => x.IFSC)
    </div>
    <div class="form-group">
        <label>AccountType</label>
        @Html.TextBoxFor(x => x.AccountType)
    </div>
    <div class="form-group">
        <button type="submit">Submit</button>
    </div>
</div>


}


